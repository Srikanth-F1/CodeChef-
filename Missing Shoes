]import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	    Scanner sri=new Scanner(System.in);
		int r=sri.nextInt();
		int l=sri.nextInt();
		if((r-l)<0)
		System.out.println(l-r);
		else
		System.out.println(r-l);
		
	}
}
